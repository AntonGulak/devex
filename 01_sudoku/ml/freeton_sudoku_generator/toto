//private (intermediary) variables

blueprint_variable<field_type> aux_23;
blueprint_variable<field_type> aux_22;
blueprint_variable<field_type> aux_21;
blueprint_variable<field_type> aux_20;
blueprint_variable<field_type> aux_19;
blueprint_variable<field_type> aux_18;
blueprint_variable<field_type> aux_17;
blueprint_variable<field_type> aux_16;
blueprint_variable<field_type> aux_15;
blueprint_variable<field_type> aux_14;
blueprint_variable<field_type> aux_13;
blueprint_variable<field_type> aux_12;
blueprint_variable<field_type> aux_11;
blueprint_variable<field_type> aux_10;
blueprint_variable<field_type> aux_9;
blueprint_variable<field_type> aux_8;
blueprint_variable<field_type> aux_7;
blueprint_variable<field_type> aux_6;
blueprint_variable<field_type> aux_5;
blueprint_variable<field_type> aux_4;
blueprint_variable<field_type> aux_3;
blueprint_variable<field_type> aux_2;
blueprint_variable<field_type> aux_1;
blueprint_variable<field_type> aux_0;
 //public variables

blueprint_variable<field_type> x_0_0;
blueprint_variable<field_type> x_0_1;
blueprint_variable<field_type> x_0_2;
blueprint_variable<field_type> x_0_3;
blueprint_variable<field_type> x_1_0;
blueprint_variable<field_type> x_1_1;
blueprint_variable<field_type> x_1_2;
blueprint_variable<field_type> x_1_3;
blueprint_variable<field_type> x_2_0;
blueprint_variable<field_type> x_2_1;
blueprint_variable<field_type> x_2_2;
blueprint_variable<field_type> x_2_3;
blueprint_variable<field_type> x_3_0;
blueprint_variable<field_type> x_3_1;
blueprint_variable<field_type> x_3_2;
blueprint_variable<field_type> x_3_3;
 
void generate_r1cs_constraints() {

bp.add_r1cs_constraint(r1cs_constraint<field_type>(x_1_1, x_1_2, aux_0));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_0, x_2_1, aux_1));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_1, x_2_2, 10 * one));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(x_1_0, x_1_1, aux_2));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_2, x_2_0, aux_3));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_3, x_2_1, 10 * one));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(x_0_1, x_0_2, aux_4));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_4, x_1_1, aux_5));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_5, x_1_2, 10 * one));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(x_0_0, x_0_1, aux_6));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_6, x_1_0, aux_7));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_7, x_1_1, 10 * one));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(x_0_3, x_1_3, aux_8));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_8, x_2_3, aux_9));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_9, x_3_3, 10 * one));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(x_0_2, x_1_2, aux_10));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_10, x_2_2, aux_11));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_11, x_3_2, 10 * one));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(x_0_1, x_1_1, aux_12));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_12, x_2_1, aux_13));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_13, x_3_1, 10 * one));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(x_0_0, x_1_0, aux_14));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_14, x_2_0, aux_15));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_15, x_3_0, 10 * one));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(x_3_0, x_3_1, aux_16));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_16, x_3_2, aux_17));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_17, x_3_3, 10 * one));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(x_2_0, x_2_1, aux_18));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_18, x_2_2, aux_19));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_19, x_2_3, 10 * one));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(x_1_0, x_1_1, aux_20));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_20, x_1_2, aux_21));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_21, x_1_3, 10 * one));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(x_0_0, x_0_1, aux_22));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_22, x_0_2, aux_23));
bp.add_r1cs_constraint(r1cs_constraint<field_type>(aux_23, x_0_3, 10 * one));
}